{
    "version": 2,
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.7.0" }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "outputDirectory": "public",
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://restaurante-chaqueno.vercel.app"
    }
}